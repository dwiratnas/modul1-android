<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="e.android.dwiratna_1202150258_modul1.SecondActivity">

    <TextView
        android:id="@+id/id_makanmalam"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:text="Makan Malam"
        android:textSize="35dp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="25dp" />

    <TextView
        android:id="@+id/id_tempat"
        android:layout_width="match_parent"
        android:layout_height="28dp"
        android:layout_marginTop="8dp"
        android:gravity="center"
        android:textSize="20dp"
        app:layout_constraintTop_toBottomOf="@+id/id_makanmalam"
        tools:layout_editor_absoluteX="0dp" />

    <TextView
        android:id="@+id/id_menu"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:gravity="center"
        android:text="Menu"
        android:textSize="18dp"
        app:layout_constraintTop_toBottomOf="@+id/id_tempat"
        tools:layout_editor_absoluteX="0dp" />

    <TextView
        android:id="@+id/id_menu1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:gravity="center"
        android:textSize="35dp"
        app:layout_constraintTop_toBottomOf="@+id/id_menu"
        tools:layout_editor_absoluteX="154dp" />

    <TextView
        android:id="@+id/id_porsi"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:gravity="center"
        android:text="porsi"
        android:textSize="18dp"
        app:layout_constraintTop_toBottomOf="@+id/id_menu1"
        tools:layout_editor_absoluteX="146dp" />

    <TextView
        android:id="@+id/id_porsi1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:gravity="center"
        android:textSize="35dp"
        app:layout_constraintTop_toBottomOf="@+id/id_porsi"
        tools:layout_editor_absoluteX="146dp" />

    <TextView
        android:id="@+id/id_harga"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:gravity="center"
        android:text="harga"
        android:textSize="18dp"
        app:layout_constraintTop_toBottomOf="@+id/id_porsi1"
        tools:layout_editor_absoluteX="146dp" />

    <TextView
        android:id="@+id/id_harga1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:gravity="center"
        android:textSize="35dp"
        app:layout_constraintTop_toBottomOf="@+id/id_harga"
        tools:layout_editor_absoluteX="146dp" />


</android.support.constraint.ConstraintLayout>
